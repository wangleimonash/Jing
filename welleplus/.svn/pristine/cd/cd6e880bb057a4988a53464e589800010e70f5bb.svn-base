package com.welleplus.enums;

public enum Role {
	PROMANAG("项目管理员",1),BIDMANAG("标段管理员",2),PROJECTMANAG("工程管理员",3),DEPAMANAG("部门管理员",4),GENSTAFF("普通员工",5);
	private Role(String name,int value){
		this.name = name;
		this.value = value;
	}
	private String name;
	private int value;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}
	

}
