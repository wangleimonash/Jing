                    <div class="col-xs-12">
                        <!--<div class="clearfix" style="background-color: #bbb; height: 3px"></div>-->
                        <div class="clearfix" style="height: 8px"></div>
                        <div style="min-height: 390px;overflow:auto">
                            <table class="table table-striped table-bordered" style="width:1100px">
                                <thead>
                                <tr>
                                    <th>序号</th>
                                    <th>姓名</th>
                                    <th>工种</th>
                                    <th>电话</th>
                                    <th>设备类型</th>
                                    <th>设备编号</th>
                                    <th>电量</th>
                                    <th>定位时间</th>
<!--                                     <th>经度</th> -->
<!--                                     <th>纬度</th> -->
                                    <th>地址</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr ng-repeat="t in equipInfos">
                                    <td>{{10*indexPage+$index+1}}</td>
                                    <td>{{t.name}}</td>
                                    <td>{{t.worktypeStr}}</td>
                                    <td>{{t.phonenumber}}</td>
                                    <td>{{t.equipTypeStr}}</td>
                                    <td>{{t.equipnumber}}</td>
                                    <td>{{t.Bat}}</td>
                                    <td>{{t.watchDateStr}}</td>
<!--                                     <td>{{t.Xloc}}</td> -->
<!--                                     <td>{{t.Yloc}}</td> -->
                                    <td>{{t.address}}</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
