<div>
	<div class="col-md-6 col-sm-6 col-xs-12">
		<div class="x_panel" style="width:100%;height:400px">
	        <div class="x_title">
	          <h2>各标段工种信息<small></small></h2>
	          <div class="clearfix"></div>
	        </div>
		        <div id="mybarChart" style="width:100%;height:313px" class="container">
        		</div>
	      </div>
    </div>
    
    <div class="col-md-6 col-sm-6 col-xs-12">
		<div class="x_panel" style="width:100%;height:400px">
	        <div class="x_title">
	          <h2>设备类型占比<small></small></h2>
	          <div class="clearfix"></div>
	        </div>
		        <div id="equipTypeChart" style="width:100%;height:313px" class="container">
	        	</div>
	      </div>
    </div>
    
    
    <div class="col-md-7 col-sm-6 col-xs-12">
		<div class="x_panel" style="width:100%;height:400px">
	        <div class="x_title">
	          <h2>电子围栏信息 <small></small></h2>
	          <div class="clearfix"></div>
	        </div>
	        <div class="x_content" style="text-align:center">
				<div id="container" style="height:313px" class="container">
        		</div>
	        </div>
	      </div>
    </div>
    
    <div class="col-md-5 col-sm-6 col-xs-12">
		<div class="x_panel" style="width:100%;height:400px">
	        <div class="x_title">
	          <h2>天气 <small></small></h2>
	          <div class="clearfix"></div>
	        </div>
	        <div class="x_content" style="text-align:center">
	          <div class="row">
	            <div class="col-sm-12">
	              <div class="temperature" style="font-size:18px;margin-left:61px">{{localtime}}<b></b>  &nbsp;&nbsp;星期{{dayofweeks[0]}}
<!-- 	                <span>F</span> -->
<!-- 	                <span><b>C</b></span> -->
	              </div>
	            </div>
	          </div>
	          <div class="row">
	            <div class="col-sm-4">
	              <div class="weather-icon">
	                <canvas height="84" width="84" id="partly-cloudy-day"></canvas>
	              </div>
	            </div>
	            <div class="col-sm-8">
	              <div class="weather-text" style="text-align:center;margin-top:35px">
	                <span style="font-size:24px;margin-right:60px">{{address}}<br><i></i></span>
	              </div>
	            </div>
	          </div>
	          <div class="col-sm-12">
	            <div class="weather-text pull-right">
	              <h3 class="degrees">23</h3>
	            </div>
	          </div>
	
	          <div class="clearfix"></div>
	
	          <div class="row weather-days">
	            <div class="col-sm-2">
	              <div class="daily-weather">
	                <h2 class="day">{{dayofweeks[1]}}</h2>
	                <h3 class="degrees">25</h3>
	                <canvas id="clear-day" width="32" height="32"></canvas>
	                <h5>15 <i>km/h</i></h5>
	              </div>
	            </div>
	            <div class="col-sm-2">
	              <div class="daily-weather">
	                <h2 class="day">{{dayofweeks[2]}}</h2>
	                <h3 class="degrees">25</h3>
	                <canvas height="32" width="32" id="rain"></canvas>
	                <h5>12 <i>km/h</i></h5>
	              </div>
	            </div>
	            <div class="col-sm-2">
	              <div class="daily-weather">
	                <h2 class="day">{{dayofweeks[3]}}</h2>
	                <h3 class="degrees">27</h3>
	                <canvas height="32" width="32" id="snow"></canvas>
	                <h5>14 <i>km/h</i></h5>
	              </div>
	            </div>
	            <div class="col-sm-2">
	              <div class="daily-weather">
	                <h2 class="day">{{dayofweeks[4]}}</h2>
	                <h3 class="degrees">28</h3>
	                <canvas height="32" width="32" id="sleet"></canvas>
	                <h5>15 <i>km/h</i></h5>
	              </div>
	            </div>
	            <div class="col-sm-2">
	              <div class="daily-weather">
	                <h2 class="day">{{dayofweeks[5]}}</h2>
	                <h3 class="degrees">28</h3>
	                <canvas height="32" width="32" id="wind"></canvas>
	                <h5>11 <i>km/h</i></h5>
	              </div>
	            </div>
	            <div class="col-sm-2">
	              <div class="daily-weather">
	                <h2 class="day">{{dayofweeks[6]}}</h2>
	                <h3 class="degrees">26</h3>
	                <canvas height="32" width="32" id="cloudy"></canvas>
	                <h5>10 <i>km/h</i></h5>
	              </div>
	            </div>
	            <div class="clearfix"></div>
	          </div>
	        </div>
	      </div>
    </div>
</div>
