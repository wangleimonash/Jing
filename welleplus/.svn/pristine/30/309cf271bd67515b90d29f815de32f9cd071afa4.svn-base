<!-- 	查询出勤位置信息 -->
	<select id="getWorkPosition" parameterType="Map" resultType="Map">
	    select imei,bat,DATE_FORMAT(watch_date,'%Y-%m-%d %H:%i:%s') as watch_date,address from map_watch_data_act 
	    
<!-- 	    where id in (select min(id) from map_watch_data_act where imei = #{imei} and area = 1 group by to_days(watch_date)) -->
	    <where>
	        id in (select min(id) from map_watch_data_act where imei = #{imei} and area = 1 
	        <if test="startDate!=null and startDate!=''">
	            and watch_date >= #{startDate}
	        </if>
	        <if test="endDate!=null and endDate!=''">
	            and watch_date &lt;= #{endDate}
	        </if>
	        group by to_days(watch_date))
	    </where>
	   
	</select>

